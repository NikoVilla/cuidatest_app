{
  "extends": "expo/tsconfig.base",
  "compilerOptions": {
    "jsx": "react",
    "allowJs": true,
    "checkJs": true,
    "isolatedModules": true,
    "moduleResolution": "node",
    "target": "esnext",
    "strict": true,
    "noEmit": true,
    "skipLibCheck": true,
    "esModuleInterop": true,
    "forceConsistentCasingInFileNames": true,
    "paths": {
      "@/*": [
        "./*"
      ]
    }
  },
  "include": [
    "**/*.ts",
    "**/*.tsx",
    ".expo/types/**/*.ts",
    "expo-env.d.ts"
, "app/_layout.tsx", "app/index.jsx", "app/signUp/index.jsx", "components/Contacts/Card.tsx"  ]
}
